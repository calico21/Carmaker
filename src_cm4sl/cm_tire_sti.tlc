%implements "cm_tire_sti" "C"

%include "cm_rtwlib.tlc"


%function BlockTypeSetup(block, system) void
    %<LibAddToCommonIncludes("<Global.h>")>
    %<LibAddToCommonIncludes("<Log.h>")>
    %<LibAddToCommonIncludes("<Vehicle.h>")>
    %<LibAddToCommonIncludes("<Vehicle/Tire.h>")>
%endfunction


%function Start (block, system) Output
    %assign tirepos = block.SFcnParamSettings[0].TirePos
    %assign tireid = CAST("Number", block.SFcnParamSettings[0].TireId)
#ifndef TIRE_%<tirepos>
#   define TIRE_%<tirepos>
    %if CM_IsCarMakerTarget
	if (VhclModel_Tire_Check(%<tireid>, 1) == 0) {
	    %<LibBlockPWork(Tire, "", "", 0)> = VhclModel_Tire_GetHandle(%<tireid>);
	} else {
	    %<LibBlockPWork(Tire, "", "", 0)> = NULL;
	}
    %else
	%<LibBlockPWork(TireIF, "", "", 0)> = NULL;
    %endif
#else
#   error Tire %<tirepos>: Only one tire block per position permitted
#endif
%endfunction


%function Terminate (block, system) Output
    %if CM_IsRtiTarget || CM_IsDsRtTarget
	{
	    tTireIF *IF = %<LibBlockPWork(TireIF, "", "", 0)>;
	    if (IF != NULL)
		free(IF);
	}
    %endif
%endfunction


%function Outputs (block, system) Output
    %if CM_IsCarMakerTarget
	{
	    tTire *tire = %<LibBlockPWork(Tire, "", "", 0)>;
	    if (tire != NULL) {
		tTireIF *IF = &tire->IF;

		IF->WC_t_0[0] = %<LibBlockInputSignal (0, "", "", 0)>;
		IF->WC_t_0[1] = %<LibBlockInputSignal (0, "", "", 1)>;
		IF->WC_t_0[2] = %<LibBlockInputSignal (0, "", "", 2)>;

		IF->WC_vel_2[0] = %<LibBlockInputSignal (1, "", "", 0)>;
		IF->WC_vel_2[1] = %<LibBlockInputSignal (1, "", "", 1)>;
		IF->WC_vel_2[2] = %<LibBlockInputSignal (1, "", "", 2)>;

		IF->WC_omega_2[0] = %<LibBlockInputSignal (2, "", "", 0)>;
		IF->WC_omega_2[1] = %<LibBlockInputSignal (2, "", "", 1)>;
		IF->WC_omega_2[2] = %<LibBlockInputSignal (2, "", "", 2)>;

		IF->Rim_rot  = %<LibBlockInputSignal (3, "", "", 0)>;

		IF->Rim_rotv = %<LibBlockInputSignal (4, "", "", 0)>;

		IF->WC_Tr2Fr0[0][0] = %<LibBlockInputSignal (5, "", "", 0)>;
		IF->WC_Tr2Fr0[0][1] = %<LibBlockInputSignal (5, "", "", 1)>;
		IF->WC_Tr2Fr0[0][2] = %<LibBlockInputSignal (5, "", "", 2)>;
		IF->WC_Tr2Fr0[1][0] = %<LibBlockInputSignal (5, "", "", 3)>;
		IF->WC_Tr2Fr0[1][1] = %<LibBlockInputSignal (5, "", "", 4)>;
		IF->WC_Tr2Fr0[1][2] = %<LibBlockInputSignal (5, "", "", 5)>;
		IF->WC_Tr2Fr0[2][0] = %<LibBlockInputSignal (5, "", "", 6)>;
		IF->WC_Tr2Fr0[2][1] = %<LibBlockInputSignal (5, "", "", 7)>;
		IF->WC_Tr2Fr0[2][2] = %<LibBlockInputSignal (5, "", "", 8)>;

		VhclModel_Tire_Calc(SimCore.DeltaT, tire);

		%<LibBlockOutputSignal(0, "", "",  0)> = IF->Frc_C[0];
		%<LibBlockOutputSignal(0, "", "",  1)> = IF->Frc_C[1];
		%<LibBlockOutputSignal(0, "", "",  2)> = IF->Frc_C[2];
		%<LibBlockOutputSignal(0, "", "",  3)> = IF->Trq_C[0];
		%<LibBlockOutputSignal(0, "", "",  4)> = IF->Trq_C[1];
		%<LibBlockOutputSignal(0, "", "",  5)> = IF->Trq_C[2];
		%<LibBlockOutputSignal(0, "", "",  6)> = IF->Frc_W[0];
		%<LibBlockOutputSignal(0, "", "",  7)> = IF->Frc_W[1];
		%<LibBlockOutputSignal(0, "", "",  8)> = IF->Frc_W[2];
		%<LibBlockOutputSignal(0, "", "",  9)> = IF->Trq_W[0];
		%<LibBlockOutputSignal(0, "", "", 10)> = IF->Trq_W[1];
		%<LibBlockOutputSignal(0, "", "", 11)> = IF->Trq_W[2];
		%<LibBlockOutputSignal(0, "", "", 12)> = IF->Slp;
		%<LibBlockOutputSignal(0, "", "", 13)> = IF->Alpha;
		%<LibBlockOutputSignal(0, "", "", 14)> = IF->TurnSlp;
		%<LibBlockOutputSignal(0, "", "", 15)> = IF->InclinAngle;
		%<LibBlockOutputSignal(0, "", "", 16)> = IF->P_v0_W[0];
		%<LibBlockOutputSignal(0, "", "", 17)> = IF->P_v0_W[1];
		%<LibBlockOutputSignal(0, "", "", 18)> = IF->P_v0_W[2];
		%<LibBlockOutputSignal(0, "", "", 19)> = IF->vBelt;
		%<LibBlockOutputSignal(0, "", "", 20)> = IF->rBelt_eff;
		%<LibBlockOutputSignal(0, "", "", 21)> = IF->Radius;
		%<LibBlockOutputSignal(0, "", "", 22)> = IF->muRoad;
		%<LibBlockOutputSignal(0, "", "", 23)> = IF->P_0[0];
		%<LibBlockOutputSignal(0, "", "", 24)> = IF->P_0[1];
		%<LibBlockOutputSignal(0, "", "", 25)> = IF->P_0[2];
	    }
	}
    %else
	{
	    %assign tireid = CAST("Number", block.SFcnParamSettings[0].TireId)
	    if (MdlItemIndex != 0) {
		tTireIF *IF = %<LibBlockPWork(TireIF, "", "", 0)>;
		if (IF == NULL) {
		    %% Initialization once and as late as possible after program startup.
		    IF = CM_Sfun_TireGetIF(%<tireid>);
		    %<LibBlockPWork(TireIF, "", "", 0)> = IF;
		}
		IF->WC_t_0[0] = %<LibBlockInputSignal (0, "", "", 0)>;
		IF->WC_t_0[1] = %<LibBlockInputSignal (0, "", "", 1)>;
		IF->WC_t_0[2] = %<LibBlockInputSignal (0, "", "", 2)>;

		IF->WC_vel_2[0] = %<LibBlockInputSignal (1, "", "", 0)>;
		IF->WC_vel_2[1] = %<LibBlockInputSignal (1, "", "", 1)>;
		IF->WC_vel_2[2] = %<LibBlockInputSignal (1, "", "", 2)>;

		IF->WC_omega_2[0] = %<LibBlockInputSignal (2, "", "", 0)>;
		IF->WC_omega_2[1] = %<LibBlockInputSignal (2, "", "", 1)>;
		IF->WC_omega_2[2] = %<LibBlockInputSignal (2, "", "", 2)>;

		IF->Rim_rot  = %<LibBlockInputSignal (3, "", "", 0)>;

		IF->Rim_rotv = %<LibBlockInputSignal (4, "", "", 0)>;

		IF->WC_Tr2Fr0[0][0] = %<LibBlockInputSignal (5, "", "", 0)>;
		IF->WC_Tr2Fr0[0][1] = %<LibBlockInputSignal (5, "", "", 1)>;
		IF->WC_Tr2Fr0[0][2] = %<LibBlockInputSignal (5, "", "", 2)>;
		IF->WC_Tr2Fr0[1][0] = %<LibBlockInputSignal (5, "", "", 3)>;
		IF->WC_Tr2Fr0[1][1] = %<LibBlockInputSignal (5, "", "", 4)>;
		IF->WC_Tr2Fr0[1][2] = %<LibBlockInputSignal (5, "", "", 5)>;
		IF->WC_Tr2Fr0[2][0] = %<LibBlockInputSignal (5, "", "", 6)>;
		IF->WC_Tr2Fr0[2][1] = %<LibBlockInputSignal (5, "", "", 7)>;
		IF->WC_Tr2Fr0[2][2] = %<LibBlockInputSignal (5, "", "", 8)>;

		CM_Sfun_TireCalc(%<tireid>);
	    }
	    {
		const tTireIF *IF;
		if (MdlItemIndex == 0) {
		    IF = MdlZero.TireIF;
		} else {
		    IF = %<LibBlockPWork(TireIF, "", "", 0)>;
		}
		%<LibBlockOutputSignal(0, "", "",  0)> = IF->Frc_C[0];
		%<LibBlockOutputSignal(0, "", "",  1)> = IF->Frc_C[1];
		%<LibBlockOutputSignal(0, "", "",  2)> = IF->Frc_C[2];
		%<LibBlockOutputSignal(0, "", "",  3)> = IF->Trq_C[0];
		%<LibBlockOutputSignal(0, "", "",  4)> = IF->Trq_C[1];
		%<LibBlockOutputSignal(0, "", "",  5)> = IF->Trq_C[2];
		%<LibBlockOutputSignal(0, "", "",  6)> = IF->Frc_W[0];
		%<LibBlockOutputSignal(0, "", "",  7)> = IF->Frc_W[1];
		%<LibBlockOutputSignal(0, "", "",  8)> = IF->Frc_W[2];
		%<LibBlockOutputSignal(0, "", "",  9)> = IF->Trq_W[0];
		%<LibBlockOutputSignal(0, "", "", 10)> = IF->Trq_W[1];
		%<LibBlockOutputSignal(0, "", "", 11)> = IF->Trq_W[2];
		%<LibBlockOutputSignal(0, "", "", 12)> = IF->Slp;
		%<LibBlockOutputSignal(0, "", "", 13)> = IF->Alpha;
		%<LibBlockOutputSignal(0, "", "", 14)> = IF->TurnSlp;
		%<LibBlockOutputSignal(0, "", "", 15)> = IF->InclinAngle;
		%<LibBlockOutputSignal(0, "", "", 16)> = IF->P_v0_W[0];
		%<LibBlockOutputSignal(0, "", "", 17)> = IF->P_v0_W[1];
		%<LibBlockOutputSignal(0, "", "", 18)> = IF->P_v0_W[2];
		%<LibBlockOutputSignal(0, "", "", 19)> = IF->vBelt;
		%<LibBlockOutputSignal(0, "", "", 20)> = IF->rBelt_eff;
		%<LibBlockOutputSignal(0, "", "", 21)> = IF->Radius;
		%<LibBlockOutputSignal(0, "", "", 22)> = IF->muRoad;
		%<LibBlockOutputSignal(0, "", "", 23)> = IF->P_0[0];
		%<LibBlockOutputSignal(0, "", "", 24)> = IF->P_0[1];
		%<LibBlockOutputSignal(0, "", "", 25)> = IF->P_0[2];
	    }
	}
    %endif
%endfunction
